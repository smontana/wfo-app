                div.sidebar-menu-items

                  #homepage
                    button#homepage_.nav-group-item.btn.btn-large.btn-default.icon.icon-home 
                      | Home

                  #interactions
                    button#interactions_.nav-group-item.btn.btn-large.btn-default.icon.icon-chart-area 
                      | Interactions

                  #attendance
                    button#attendance_.nav-group-item.btn.btn-large.btn-default.icon.icon-clock 
                      | Attendance

                  #productivity
